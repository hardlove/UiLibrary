<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LuckDiskView">
        <!--外环大小-->
        <attr name="outCircleSize" format="dimension|reference" />
        <!--外环颜色-->
        <attr name="outCircleColor" format="color|reference" />
        <!--是否在外环添加闪光灯-->
        <attr name="addFlash" format="boolean" />
        <!--闪光灯激活状态颜色-->
        <attr name="flash_active_color" format="color|reference" />
        <!--闪光灯未激活状态颜色-->
        <attr name="flash_inactive_color" format="color|reference" />
        <!--是否可以旋转-->
        <attr name="enable_rotate" format="boolean" />
        <!--文字大小-->
        <attr name="textSize" format="dimension" />
        <!--是否添加分割线-->
        <attr name="add_split_line" format="boolean" />
        <attr name="split_line_size" format="dimension|reference" />


    </declare-styleable>


</resources>